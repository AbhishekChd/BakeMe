<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="11dp"
        android:left="11dp"
        android:right="11dp"
        android:top="11dp">
        <shape android:shape="oval">
            <solid android:color="#ffa726" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:innerRadius="44dp"
            android:shape="ring"
            android:thickness="10dp"
            android:useLevel="false">
            <size
                android:width="100dp"
                android:height="100dp" />
            <solid android:color="#00ffffff" />
        </shape>

    </item>


    <item
        android:bottom="0.00001dip"
        android:left="0.00001dip"
        android:right="0.00001dip"
        android:top="0.00001dip">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:innerRadius="48dp"
            android:shape="ring"
            android:thickness="2dp"
            android:useLevel="false">
            <size
                android:width="100dp"
                android:height="100dp" />
            <solid android:color="@color/colorPrimary" />
        </shape>

    </item>

</layer-list>